{
  "name": "@damenluo/sentry-miniapp",
  "version": "10.27.0-rc.1",
  "description": "用于小程序/小游戏平台的 Sentry SDK",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/junqing258/sentry-miniapp.git"
  },
  "homepage": "https://github.com/junqing258/sentry-miniapp#readme",
  "bugs": {
    "url": "https://github.com/junqing258/sentry-miniapp/issues"
  },
  "license": "BSD-3-Clause",
  "author": "junqing258",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "miniprogram": "dist",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "./src/polyfills/*.ts",
    "./dist/index.js",
    "./dist/index.mjs",
    "./dist/index.cjs"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "vite build --minify terser",
    "dev:weapp": "vite build --mode dev --watch --outDir examples/weapp/libs/sentry",
    "prepack": "vite build",
    "lint": "tsc --noEmit",
    "version": "node ./scripts/versionbump.js src/version.ts"
  },
  "keywords": [
    "sentry",
    "miniprogram",
    "miniapp",
    "weapp",
    "wechat",
    "alipay",
    "bytedance",
    "baidu",
    "dingtalk",
    "taro",
    "error-tracking",
    "monitoring",
    "tracing",
    "performance",
    "apm"
  ],
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@swc/core": "^1.15.3",
    "@types/node": "^12.7.1",
    "change-case": "^5.4.4",
    "install": "^0.13.0",
    "minimist": "^1.2.8",
    "miniprogram-api-typings": "^4.0.4",
    "replace-in-file": "^8.3.0",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "vite": "^7.2.0",
    "vite-plugin-dts": "^4.5.4"
  },
  "dependencies": {
    "@sentry/core": "10.27.0"
  }
}